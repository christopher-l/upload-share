{
	"name": "Upload Share",
	"short_name": "Upload Share",
	"start_url": "./",
	"display": "standalone",
	"background_color": "#ffffff",
	"lang": "en",
	"scope": "./",
	"description": "Upload stuff to your Raspberry Pi",
	"theme_color": "#e1e6eb",
	"icons": [
		{ "src": "favicon-192.png", "sizes": "192x192", "type": "image/png" },
		{ "src": "favicon-512.png", "sizes": "512x512", "type": "image/png" }
	],
	"share_target": {
		"action": "/?/upload",
		"method": "POST",
		"enctype": "multipart/form-data",
		"params": { "files": [{ "name": "files", "accept": ["*/*", ".*"] }] }
	}
}
