{
	"name": "Upload Share",
	"short_name": "Upload Share",
	"start_url": "./",
	"display": "standalone",
	"background_color": "#ffffff",
	"lang": "en",
	"scope": "./",
	"description": "Upload stuff to your Raspberry Pi",
	"theme_color": "#ffffff",
	"icons": [
		{ "src": "favicon-1024.png", "sizes": "1024x1024", "type": "image/png" },
		{
			"src": "favicon-maskable-1024.png",
			"sizes": "1024x1024",
			"type": "image/png",
			"purpose": "maskable"
		}
	],
	"share_target": {
		"action": "/pwa-share",
		"method": "POST",
		"enctype": "multipart/form-data",
		"params": { "files": [{ "name": "files", "accept": ["*/*", ".*"] }] }
	}
}
